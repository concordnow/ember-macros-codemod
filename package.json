{
  "name": "ember-macros-codemod",
  "version": "0.2.0",
  "description": "A collection of codemod's for macros.",
  "scripts": {
    "lint": "eslint --cache .",
    "test": "codemod-cli test",
    "test:coverage": "codemod-cli test --coverage",
    "update-docs": "codemod-cli update-docs",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls"
  },
  "bin": "./bin/cli.js",
  "keywords": [
    "codemod-cli",
    "codemod",
    "ember",
    "octane",
    "macros",
    "ember-macro-helpers",
    "ember-awesome-macros"
  ],
  "repository": "https://github.com/concordnow/ember-macros-codemod",
  "license": "MIT",
  "author": "Concord Worlwide Inc.",
  "dependencies": {
    "codemod-cli": "^2.1.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.6",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^24.9.0",
    "prettier": "^2.0.5"
  },
  "engines": {
    "node": ">= 11"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
